! ==================================================================================================
! Washington
! ==================================================================================================
! SSH
ssh -oKexAlgorithms=+diffie-hellman-group-exchange-sha1 -oHostKeyAlgorithms=+ssh-rsa admin@10.1.1.1

! (optional) SSH between devices
ssh -l admin address

ping 172.16.0.1
ping 172.16.0.3
ping 10.2.1.1
ping 10.3.1.1
ping 8.8.8.8

ping ipv6 FD00:7::1
ping ipv6 FD00:7::3

show ip cef 172.16.0.1
show ip cef 172.16.0.3
show ip cef 10.2.1.1
show ip cef 10.3.1.1
show ip cef 8.8.8.8

show ipv6 cef FD00:7::1
show ipv6 cef FD00:7::3


traceroute 8.8.8.8

! ZBFW
show zone security
show zone-pair security
show class-map type inspect
show policy-map type inspect
show policy-map type inspect zone-pair sessions

! ARP
show arp
show arp summary
show ip arp
show ip arp summary

! OSPF
show ip ospf
show ip ospf database
show ip ospf neighbor
show ip ospf neighbor detail
show ip ospf interface brief
show ip ospf topology-info

show ipv6 ospf
show ipv6 ospf database
show ipv6 ospf neighbor
show ipv6 ospf neighbor detail
show ipv6 ospf interface brief
show ipv6 ospf topology-info

! NAT
show ip nat translations
show ip nat statistics

! VPN
show dmvpn
show crypto ipsec sa
show crypto ipsec profile
show crypto ikev2 sa
show crypto ikev2 profile

! NHRP
show ip nhrp
show ipv6 nhrp

! Bandwidth
show platform hardware throughput level

! SNMP
show snmp
show snmp user


